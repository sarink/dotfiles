alias gowsn="cd $HOME/Projects/worldsingles/"

eros_refresh() {
  docker-machine stop worldsingles
  docker-machine start worldsingles
  cd $HOME/Projects/worldsingles/erosapp/nicheclick_erosapp/
  docker stop erosapp
  docker start erosapp
}

eros_eval() {
  eval $(docker-machine env worldsingles)
}

alias eros_login="docker start erosapp && docker exec -it erosapp bash"
